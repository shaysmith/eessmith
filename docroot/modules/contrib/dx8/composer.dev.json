{
    "name": "cohesion/dx8",
    "description": "DX8 module",
    "version": "5.5.6",
    "type": "drupal-module",
    "minimum-stability": "dev",
    "extra": {
        "drush": {
            "services": {
                "drush.services.yml": "^9"
            }
        },
        "patches": {
            "drupal/entity_reference_revisions": {
                "'Create new revision' option is ignored when updating EntityReferenceRevisionsItem": "https://www.drupal.org/files/issues/2019-06-27/3025709-30.patch"
            }
        }
    },
    "require": {
        "cweagans/composer-patches": "^1.6",
        "drupal/entity_reference_revisions": "^1.6",
        "drupal/imce": "^1.7",
        "drupal/token": "^1.5",
        "drupal/admin_toolbar": "^1.27",
        "drupal/context": "^4.0",
        "drupal/google_analytics": "^2.4",
        "drupal/views_infinite_scroll": "^1.5",
        "drupal/request_data_conditions": "dev-master",
        "drupal/search_api": "^1.13",
        "drupal/menu_item_extras": "2.5",
        "drupal/menu_link_attributes": "^1.0",
        "drupal/entity_browser": "^2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0|^5.0"
    }
}
